            <&key_physical_attrs 100 100  600    400       0     0     0>;
    };

    krankboard_r_position_map {
        compatible = "zmk,physical-layout-position-map";
        complete;

        krankboard_r_map: krankboard_r {
            physical-layout = <&physical_layout0>;
